func Merge (a,b)
	d = a.attrList()
	e = b.attrList()
	for attr in d 
		if ( type (a[attr]) == "Json" && type (b) == "Json" )
			if (attr in e )
				print ("hello") 
			
			end
		else
			print ("world")
		
		end
	end
	
	print ("a")
